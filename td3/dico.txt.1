	require(['jquery'], function($) {

	var liveStatConfig = {
		jsonUrl: "https://arche.univ-lorraine.fr/live-stats/stats.json", 
		divId: "#live-mdl-stats",
		refreshTime: 120000,
		cssFile: "https://arche.univ-lorraine.fr/live-stats/live-mdl-stats.css"
	};
		
	(function() {
			
		function formatnumber(number) { 
			var n = ''+number;
			var r ='';
			var count=0;
			
			for(var i=n.length-1 ; i>=0 ; i--) {
				if(count!=0 && count % 3 == 0)
					r = n[i]+' '+r ;
				else
					r = n[i]+r ;
				count++;
			}		
			return(r)
		}
		
		function getdata() { 		
			$.getJSON( liveStatConfig.jsonUrl, function( data, status ) {
				if (status === "success") {	
					var divLive = $('<div/>', {
						id: 'live-user',					
					});	
					$(divLive).append("<span class='directlive-user'>"+formatnumber(data.liveuser)+"</span>");				
					$(liveStatConfig.divId).html( divLive );				
					
					var x = new Date();
					x.setDate(1);
					x.setMonth(x.getMonth()-1);
					var month = new Array();
					month[0] = "janvier";
					month[1] = "février";
					month[2] = "mars";
					month[3] = "avril";
					month[4] = "mai";
					month[5] = "juin";
					month[6] = "juillet";
					month[7] = "août";
					month[8] = "septembre";
					month[9] = "octobre";
					month[10] = "novembre";
					month[11] = "décembre";
					var monthText = month[x.getMonth()]; 
					
					var sleep = 0;
					$(divLive).mouseenter(function() {
						sleep = 1;
						$(divLive).mouseleave(function() {
							sleep = 0;
							updatedata();
						});
						var ob = $(this);
						setTimeout(function() {                         
							if(sleep) {
								$(divLive).html( "" );
								$(divLive).append("<br /><span class='directlive-user'><b>"+formatnumber(data.liveuser)+"</b> utilisateurs en ligne</span><br />");
								if (data.yesterdayconnection >0) {
									$(divLive).append("<span class='yesterday-connection'><b>"+formatnumber(data.yesterdayconnection)+"</b> connexions hier par <b>"+formatnumber(data.yesterdayuser)+"</b> utilisateurs différents</span>");
								}
								$(divLive).append("<span class='lastmonth-connection'><b>"+formatnumber(data.lastmonthconnection)+"</b> connexions en "+monthText+" par <b>"+formatnumber(data.lastmonthuser)+"</b> utilisateurs différents</span>");
								$(divLive).append("<span class='lastupdate'>Dernière mise à jour : "+data.lastupdate+"</span>");
								$(divLive).addClass("ishover");					
							}
						}, 700);
					});

				}
			});
		}

		function updatedata() {
			getdata();
			setTimeout(updatedata, liveStatConfig.refreshTime)
		}

		$(document).ready(function(){
